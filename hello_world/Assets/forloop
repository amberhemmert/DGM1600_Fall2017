A For Loop is given different statements and will verify that they are true, before executing. 

public class For_Loop : MonoBehaviour {
	public int bottles = 100;
	// Use this for initialization
	void Start () {
	// 	while(bottles > 0) 
	// 	print(bottles + " of beer on the wall");
	// 	bottles = bottles -- ;
	// }
	for(;bottles > 0; bottles --){
		print (bottles + " of beer on the wall");
	}
}
	// Update is called once per frame
	void Update () {
		
	}
}

	public int gum = 10;
	// Use this for initialization
	void Start () {
	// 	while(gum > 0) 
	// 	print(gum + " sticks of gum in the pack");
	// 	gum = gum -- ;
	// }
	for(;gum > 0; gum --){
		print (gum + " sticks of gum in the pack");
	}
}
	// Update is called once per frame
	void Update () {
		
	}
}

}

